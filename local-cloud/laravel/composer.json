{
    "name": "pinleague/Tailwind",
    "description": "Tailwind - The beaveriest app",
    "license":"proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pinleague/mustache-l4"
        },
        {
            "type": "vcs",
            "url": "https://github.com/pinleague/intercom-php"
        },
        {
            "type": "vcs",
            "url": "https://github.com/pinleague/fastimage"
        },
        {
            "type": "vcs",
            "url": "https://github.com/pinleague/guzzle3.git"
        }
    ],
    "require": {
        "algolia/algoliasearch-client-php": "dev-master",
        "conarwelsh/mustache-l4": "dev-master",
        "dg/rss-php": "dev-master",
        "fastimage/fastimage": "dev-master",
        "gchartphp/gchartphp": "*",
        "guzzle/guzzle": "~3.7",
        "hipchat/hipchat-php": "> 1.0.0",
        "kevinlebrun/colors.php": "0.*",
        "laravel/framework": "4.1.*",
        "mailchimp/mailchimp": "2.*",
        "mailgun/mailgun-php": "*",
        "nesbot/Carbon": "*",
        "nubera-ebusiness/intercom-php": "dev-master",
        "phpmailer/phpmailer": "*",
        "roumen/sitemap": "dev-master",
        "segmentio/analytics-php":"dev-master",
        "symfony/dom-crawler":"*",
        "willdurand/geocoder": "2.4.2",
        "willwashburn/HTML-Strap-PHP": "*",
        "yzalis/ua-parser": "0.1.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "Mockery/Mockery": ">=0.7.2",
        "psy/psysh": "0.1.0.*@dev",
        "aura/sqlschema": "dev-master"
    },
    "autoload": {
        "classmap": [
            "app/collections",
            "app/commands",
            "app/controllers",
            "app/database/migrations",
            "app/database/seeds",
            "app/lib/",
            "app/models",
            "app/tests/TestCase.php"
        ],
        "psr-4": {
            "Collections\\": "app/collections",
            "Composers\\": "app/views/Composers",
            "Models\\": "app/models"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
